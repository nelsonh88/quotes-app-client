

{{#each users as |user|}}

      <div class="user">
        <span>Submitted by</span>
        <a href="javascript:void(0)">{{user.first_name}} {{user.last_name}}</a>
      </div>
    {{#each user.quotes as |quote|}}
    <div class="quote-block">
      {{#if quote}}
      <button class="add-quote btn btn-dark" data-quote="{{quote.quote}}" data-author="{{quote.author}}" data-id={{quote.id}}>
        Add To MyQuotes
      </button>
        <p class="quote">{{quote.quote}}</p>
        <p class="author">{{quote.author}}</p>
      {{else}}
        <p>User has no quotes</p>
      {{/if}}
    </div>
    {{/each}}
{{/each}}
